{
  "name": "angular-froala-wysiwyg-base",
  "version": "3.0.6",
  "description": "Angular 2, 4, 5, 6, 7 and 8 bindings for Froala WYSIWYG HTML rich text editor",
  "main": "bundles/angular-froala-wysiwyg.umd.js",
  "typings": "index.d.ts",
  "module": "index.js",
  "scripts": {
    "lite-server": "lite-server -c demo/bs-config.json",
    "demo.serve": "npm run -s build link demo.build lite-server",
    "demo.build": "ng build --aot=false",
    "build": "ngm build --prod src --clean",
    "link": "ngm link -p src --here",
    "build.watch": "ngm build -p src --watch --skip-bundles",
    "start": "ng serve --aot=false --host 0.0.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/froala/angular-froala-wysiwyg.git"
  },
  "keywords": [
    "angular2",
    "angular4",
    "angular5",
    "angular6",
    "angular7",
    "angular8",
    "ng2",
    "ng4",
    "ng5",
    "ng6",
    "ng7",
    "froala",
    "html",
    "text",
    "editor",
    "wysiwyg",
    "rich editor",
    "rich text editor",
    "rte",
    "javascript"
  ],
  "author": "Froala Labs (https://www.froala.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/froala/angular-froala-wysiwyg/issues"
  },
  "dependencies": {
    "@angular-devkit/build-angular": "~0.11.0",
    "@angular/cli": "^8.0.0",
    "@angular/common": "^8.0.0",
    "@angular/compiler": "^8.0.0",
    "@angular/compiler-cli": "^8.0.0",
    "@angular/core": "^8.0.0",
    "@angular/forms": "^8.0.0",
    "@angular/http": "^7.0.0",
    "@angular/language-service": "^8.0.0",
    "@angular/platform-browser": "^8.0.0",
    "@angular/platform-browser-dynamic": "^8.0.0",
    "@angular/router": "^8.0.0",
    "@types/marked": "0.0.28",
    "@types/node": "7.0.0",
    "@types/tapable": "^0.2.5",
    "@types/webpack": "^2.2.2",
    "classlist-polyfill": "1.0.3",
    "core-js": "2.6.9",
    "font-awesome": "4.7.0",
    "froala-editor": "3.0.6",
    "ngm-cli": "^1.0.4",
    "node-sass": ">=4.0.0",
    "node-sass-tilde-importer": ">=1.0.0",
    "rxjs": "^6.3.3",
    "rxjs-compat": "^6.3.2",
    "ts-helpers": "^1.1.1",
    "ts-loader": ">=4.3.0",
    "tslib": "1.10.0",
    "typescript": "3.2.4",
    "webpack": ">=4.0.0",
    "zone.js": "^0.8.26"
  },
  "peerDependencies": {},
  "devDependencies": {}
}